{
    "name": "Advent of Code 2021",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "VARIANT": "17",
            "INSTALL_MAVEN": "true",
            "INSTALL_GRADLE": "false",
            "NODE_VERSION": "none"
        }
    },
    "settings": {
        "java.home": "/docker-java-home"
    },
    "extensions": [
		"vscjava.vscode-java-pack",
		"streetsidesoftware.code-spell-checker",
		"mhutchie.git-graph",
		"redhat.vscode-xml"
	],
    "remoteUser": "vscode",
    "features": {
        "git": "os-provided"
    }
}